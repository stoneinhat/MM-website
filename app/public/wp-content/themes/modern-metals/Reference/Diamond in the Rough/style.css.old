/* Add your custom styles here */


/* Reset and Base Styles */
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

body {
    font-family: 'Avenir', 'Avenir Next', 'Helvetica Neue', Arial, sans-serif;
    line-height: 1.6;
    color: #333;
    overflow-x: hidden;
    background: #F8F8F8;
}

.container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
}

/* Header / Navigation (Transparent Overlay) */
.header {
    position: fixed;
    top: 0;
    width: 100%;
    background: transparent;
    z-index: 1000;
    padding: 1.5rem 0;
    transition: all 0.3s ease;
}

.header.scrolled {
    background: rgba(255, 255, 255, 0.95);
    backdrop-filter: blur(10px);
    padding: 1rem 0;
}

.header.scrolled .nav-menu a,
.header.scrolled .social-icons img {
    filter: brightness(0);
}

.header-container {
    width: 100%;
    padding: 0 40px;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.header-left {
    display: flex;
    align-items: center;
    gap: 1.5rem;
}

.logo {
    flex-shrink: 0;
}

.logo img {
    height: 40px;
    width: auto;
    transition: all 0.3s ease;
}

.header.scrolled .logo img {
    filter: brightness(0);
}

.header-separator {
    width: 1px;
    height: 30px;
    background-color: #fff;
    transition: background-color 0.3s ease;
}

.header.scrolled .header-separator {
    background-color: #333;
}

.nav-menu {
    display: flex;
    justify-content: flex-start;
}

.nav-menu ul {
    display: flex;
    list-style: none;
    gap: 2.5rem;
}

.nav-menu li {
    position: relative;
}

.nav-menu a {
    text-decoration: none;
    color: #fff;
    font-family: 'Nunito Sans', sans-serif;
    font-weight: 400;
    font-size: 0.9rem;
    text-transform: uppercase;
    letter-spacing: 1px;
    transition: color 0.3s ease;
    position: relative;
    display: block;
    padding: 0.5rem 0;
}

.nav-menu a:hover {
    color: #ccc;
}

.nav-menu a::after {
    content: '';
    position: absolute;
    width: 0;
    height: 1px;
    bottom: -5px;
    left: 0;
    background-color: #fff;
    transition: width 0.3s ease;
}

.nav-menu a:hover::after {
    width: 100%;
}

.dropdown-content {
    display: none;
    position: absolute;
    background-color: rgba(255, 255, 255, 0.95);
    min-width: 200px;
    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
    z-index: 1;
    top: 100%;
    left: 0;
    border-radius: 4px;
    padding: 0.5rem 0;
    backdrop-filter: blur(10px);
}

.dropdown-content a {
    color: #333 !important;
    padding: 0.75rem 1.5rem;
    text-decoration: none;
    display: block;
    font-family: 'Nunito Sans', sans-serif;
    font-size: 0.8rem;
    transition: background-color 0.3s ease;
}

.dropdown-content a:hover {
    background-color: #f8f9fa;
    color: #333 !important;
}

.dropdown-content a::after {
    display: none;
}

.dropdown:hover .dropdown-content {
    display: block;
}

.header-right {
    display: flex;
    align-items: center;
    gap: 1.5rem;
    flex-shrink: 0;
}

.call-button {
    transition: all 0.3s ease;
}

.call-button:hover {
    transform: translateY(-2px);
}

.call-button a {
    display: inline-block;
    transition: all 0.3s ease;
}

.call-button img {
    height: 40px;
    width: auto;
    transition: all 0.3s ease;
}

.call-button:hover img {
    transform: scale(1.05);
}

.social-icons {
    display: flex;
    gap: 1rem;
}

.social-icon {
    display: inline-block;
    transition: transform 0.3s ease;
}

.social-icon:hover {
    transform: scale(1.1);
}

.social-icon img {
    height: 24px;
    width: auto;
    filter: brightness(0) invert(1);
    transition: all 0.3s ease;
}

.hamburger {
    display: none;
    flex-direction: column;
    cursor: pointer;
}

.hamburger span {
    width: 25px;
    height: 2px;
    background-color: #fff;
    margin: 3px 0;
    transition: 0.3s;
}

.header.scrolled .hamburger span {
    background-color: #333;
}

/* Hero Section */
.hero {
    height: 100vh;
    background: linear-gradient(rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0.4)), 
                url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 800"><rect width="1200" height="800" fill="%23222"/><circle cx="300" cy="200" r="50" fill="%23333"/><circle cx="900" cy="600" r="80" fill="%23333"/></svg>');
    background-size: cover;
    background-position: center;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}

/* Portfolio Hero - Refined smaller version for portfolio and other pages */
#portfolio-hero {
    height: 50vh;
    min-height: 400px;
    background: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.3));
    background-size: cover;
    background-position: center;
    background-attachment: fixed;
}

#portfolio-hero .hero-content {
    text-align: center;
    z-index: 2;
    position: relative;
    padding: 2rem;
}

#portfolio-hero .hero-title {
    font-size: 3.5rem;
    margin-bottom: 2.5rem;
    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.3);
}

#portfolio-hero .hero-contact-btn {
    background: rgba(255, 255, 255, 0.15);
    border: 2px solid rgba(255, 255, 255, 0.4);
    color: #fff;
    padding: 0.8rem 2rem;
    font-size: 0.9rem;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.1em;
    cursor: pointer;
    transition: all 0.3s ease;
    backdrop-filter: blur(15px);
    border-radius: 2px;
}

#portfolio-hero .hero-contact-btn:hover {
    background: rgba(255, 255, 255, 0.25);
    border-color: rgba(255, 255, 255, 0.6);
    transform: translateY(-2px);
    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.2);
}

#portfolio-hero .scroll-indicator {
    bottom: 1.5rem;
    opacity: 0.8;
}

.hero-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.3);
}

.hero-content {
    text-align: center;
    z-index: 2;
    position: relative;
}

.hero-title {
    font-family: 'Avenir', 'Avenir Next', 'Helvetica Neue', Arial, sans-serif;
    font-size: 4.5rem;
    font-weight: 400;
    color: #fff;
    text-transform: uppercase;
    letter-spacing: 0.4em;
    margin-bottom: 1.5rem;
    line-height: 1.1;
    text-align: center;
}

.hero-subtitle {
    font-family: 'Avenir', 'Avenir Next', 'Helvetica Neue', Arial, sans-serif;
    font-size: 1.1rem;
    font-weight: 300;
    color: #fff;
    margin-bottom: 2.5rem;
    opacity: 0.95;
    letter-spacing: 0.08em;
    text-align: center;
    max-width: 600px;
    margin-left: auto;
    margin-right: auto;
}

.hero-contact-btn {
    background: rgba(255, 255, 255, 0.1);
    border: 1px solid rgba(255, 255, 255, 0.3);
    color: #fff;
    padding: 1rem 2.5rem;
    font-family: 'Avenir', 'Avenir Next', 'Helvetica Neue', Arial, sans-serif;
    font-size: 0.85rem;
    font-weight: 500;
    text-transform: uppercase;
    letter-spacing: 0.15em;
    cursor: pointer;
    transition: all 0.3s ease;
    backdrop-filter: blur(10px);
}

.hero-contact-btn:hover {
    background: rgba(255, 255, 255, 0.2);
    border-color: rgba(255, 255, 255, 0.5);
    transform: translateY(-2px);
    box-shadow: 0 5px 20px rgba(255, 255, 255, 0.1);
}

.scroll-indicator {
    position: absolute;
    bottom: 2rem;
    left: 50%;
    transform: translateX(-50%);
    color: #fff;
    font-size: 1.5rem;
    animation: bounce 2s infinite;
}

@keyframes bounce {
    0%, 20%, 50%, 80%, 100% {
        transform: translateX(-50%) translateY(0);
    }
    40% {
        transform: translateX(-50%) translateY(-10px);
    }
    60% {
        transform: translateX(-50%) translateY(-5px);
    }
}

/* Introduction Section */
.introduction {
    min-height: 865px;
    height: auto;
    background: #fff;
    display: flex;
    flex-direction: column;
    position: relative;
    overflow: hidden;
    padding: 2rem 0;
}

.introduction .container {
    max-width: 1600px;
    padding: 3rem clamp(20px, 5vw, 80px) 2rem clamp(20px, 5vw, 80px);
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

/* Top Section: Header + Image */
.intro-top {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 8rem;
    align-items: flex-start;
    margin-bottom: 4rem;
    position: relative;
    padding: 2rem 0;
    height: 60%;
}

.intro-top::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-image: url('assets/home/section2/unnamed.png');
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
    opacity: 0.05;
    z-index: -1;
}

.intro-top > * {
    position: relative;
    z-index: 2;
}

.studio-tag {
    margin-bottom: 2rem;
}

.studio-tag span {
    font-family: 'Avenir', 'Avenir Next', 'Helvetica Neue', Arial, sans-serif;
    font-size: 0.8rem;
    font-weight: 500;
    color: #333;
    text-transform: uppercase;
    letter-spacing: 0.2em;
}

.intro-text h2 {
    font-family: 'Avenir', 'Avenir Next', 'Helvetica Neue', Arial, sans-serif;
    font-size: 2.5rem;
    font-weight: 400;
    color: #333;
    line-height: 1.3;
    margin-bottom: 1.5rem;
}

.read-more {
    font-family: 'Avenir', 'Avenir Next', 'Helvetica Neue', Arial, sans-serif;
    font-size: 0.9rem;
    color: #666;
    text-decoration: underline;
    display: inline-block;
}

.intro-image img {
    width: 100%;
    height: 450px;
    object-fit: cover;
    border-radius: 8px;
}

/* Bottom Section: Steps + Services */
.intro-bottom {
    display: grid;
    grid-template-columns: auto 1fr;
    gap: 6rem;
    align-items: flex-start;
    height: 35%;
    overflow: hidden;
}

.process-steps {
    display: flex;
    flex-direction: column;
    gap: 0.2rem;
    margin-left: 0;
}

.step-item {
    padding: 0.2rem 0.5rem;
    background: #666666;
    border-radius: 0;
    font-family: 'DM Sans', 'Avenir', 'Avenir Next', 'Helvetica Neue', Arial, sans-serif;
    font-size: 18pt;
    font-weight: 600;
    color: #ffffff;
    white-space: nowrap;
    opacity: 0;
    transform: translateY(20px);
    transition: all 0.8s ease;
    width: fit-content;
    display: inline-block;
    cursor: pointer;
    margin-bottom: 0.3rem;
}

.step-item:hover {
    background: #444444;
    transform: translateY(-2px);
}

.step-item.step-hovered,
.step-item.step-active {
    opacity: 1;
}

.step-item.step-dimmed {
    opacity: 0.2;
}

.step-item.fade-in {
    opacity: 1;
    transform: translateY(0);
}

.service-columns {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 3rem;
}

.service-column h3 {
    font-family: 'Avenir', 'Avenir Next', 'Helvetica Neue', Arial, sans-serif;
    font-size: 1.1rem;
    font-weight: 600;
    color: #333;
    margin-bottom: 1rem;
}

.service-column p {
    font-family: 'Avenir', 'Avenir Next', 'Helvetica Neue', Arial, sans-serif;
    font-size: 0.9rem;
    color: #666;
    line-height: 1.6;
    opacity: 0;
    transform: translateY(10px);
    transition: all 0.8s ease;
}

.service-column p.fade-in {
    opacity: 1;
    transform: translateY(0);
}

/* Gallery Showcase */
.gallery-showcase {
    background: #000;
    padding: 5rem 0 2rem 0;
    overflow: hidden;
    position: relative;
}

.gallery-container {
    position: relative;
    max-width: 1400px;
    margin: 0 auto;
    padding: 0 clamp(280px, 25vw, 450px) 0 20px;
}

.gallery-track {
    display: flex;
    gap: 2rem;
    transition: transform 0.5s ease;
}

.gallery-slide {
    min-width: 300px;
    height: 500px;
    flex-shrink: 0;
}

.gallery-slide img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    border-radius: 8px;
    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.3);
}

.gallery-slide .placeholder-image {
    width: 100%;
    height: 100%;
    background: #333;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    color: #fff;
    border: 1px solid #555;
}

.gallery-slide .placeholder-image i {
    font-size: 3rem;
    margin-bottom: 1rem;
    opacity: 0.7;
}

.gallery-nav {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 1rem;
}

.gallery-btn {
    background: transparent;
    color: #fff;
    border: none;
    width: 60px;
    height: 40px;
    cursor: pointer;
    transition: all 0.3s ease;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 0;
}

.gallery-btn:hover {
    transform: scale(1.1);
}

.gallery-arrow {
    width: 20px;
    height: 2px;
    background: #fff;
    position: relative;
    display: block;
    transition: all 0.3s ease;
}

.gallery-arrow::before,
.gallery-arrow::after {
    content: '';
    position: absolute;
    width: 8px;
    height: 2px;
    background: #fff;
    transition: all 0.3s ease;
}

.left-arrow::before {
    top: 0;
    left: 0;
    transform: rotate(45deg);
    transform-origin: left center;
}

.left-arrow::after {
    bottom: 0;
    left: 0;
    transform: rotate(-45deg);
    transform-origin: left center;
}

.right-arrow::before {
    top: 0;
    right: 0;
    transform: rotate(-45deg);
    transform-origin: right center;
}

.right-arrow::after {
    bottom: 0;
    right: 0;
    transform: rotate(45deg);
    transform-origin: right center;
}

.gallery-sidebar {
    position: absolute;
    right: 0;
    top: 0;
    bottom: 0;
    width: 25vw;
    min-width: 300px;
    max-width: 450px;
    background: #000;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding-left: 3rem;
    z-index: 10;
}

.sidebar-content {
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    gap: 1.5rem;
}

.sidebar-title-container {
    display: flex;
    align-items: center;
    gap: 0.5rem;
}

.sidebar-dot {
    width: 8px;
    height: 8px;
    background: #fff;
    border-radius: 50%;
}

.sidebar-title {
    color: #fff;
    font-size: 16.95pt;
    font-weight: 600;
    letter-spacing: 0.1em;
    text-transform: uppercase;
    margin: 0;
    font-family: 'DM Sans', sans-serif;
}

.sidebar-contact-btn {
    background: transparent;
    color: #fff;
    border: 1px solid #fff;
    padding: 0.8rem 1.2rem;
    font-size: 17pt;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.05em;
    cursor: pointer;
    transition: all 0.3s ease;
    font-family: 'Avenir', 'Avenir Next', 'Helvetica Neue', Arial, sans-serif;
}

.sidebar-contact-btn:hover {
    background: #fff;
    color: #000;
}

/* Services Section - Horizontal Accordion */
.services-accordion {
    height: clamp(500px, 60vh, 800px);
    overflow: hidden;
    background: #f5f5f5;
    position: relative;
}

.accordion-container {
    display: flex;
    height: 100%;
    width: 100%;
    min-width: 300px; /* Ensures all 3 tabs (100px each) are always visible */
}

.accordion-item {
    position: relative;
    transition: flex 0.8s cubic-bezier(0.25, 0.46, 0.45, 0.94);
    flex: 0 0 100px;
    cursor: pointer;
    overflow: hidden;
    background: #ffffff;
    border-right: 1px solid #e5e7eb;
    min-width: 100px;
}

.accordion-item:last-child {
    border-right: none;
}

.accordion-item.active {
    flex: 1;
    cursor: default;
    min-width: 0;
}

.accordion-item__background {
    display: none;
}

.accordion-item__collapsed {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 3;
    opacity: 1;
    transition: opacity 0.3s ease;
    background: #ffffff;
}

.accordion-item.active .accordion-item__collapsed {
    opacity: 0;
    pointer-events: none;
}

.accordion-tab {
    writing-mode: vertical-rl;
    text-orientation: mixed;
    transform: rotate(180deg);
    padding: 1.5rem 0;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.accordion-tab__title {
    color: #000000;
    font-family: 'Avenir', 'Avenir Next', sans-serif;
    font-weight: 700;
    font-size: 32px;
    text-transform: uppercase;
    letter-spacing: 2px;
    transition: all 0.3s ease;
    white-space: nowrap;
}

.accordion-item:hover .accordion-tab__title {
    color: #666666;
}

.accordion-item__expanded {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 3;
    opacity: 0;
    transition: opacity 0.5s ease 0.3s;
    pointer-events: none;
    background: #ffffff;
    display: flex;
}

.accordion-item.active .accordion-item__expanded {
    opacity: 1;
    pointer-events: all;
}

.accordion-expanded__image {
    flex: 0 0 60%;
    position: relative;
    overflow: hidden;
}

.accordion-expanded__image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.accordion-expanded__details {
    flex: 0 0 40%;
    padding: 2.4rem 1.8rem;
    background: #ffffff;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.accordion-category-title {
    font-family: 'Avenir', 'Avenir Next', sans-serif;
    font-weight: 700;
    font-size: 1.5rem;
    color: #000000;
    text-transform: uppercase;
    letter-spacing: 0.15em;
    margin-bottom: 1rem;
    line-height: 1.2;
}

.accordion-expanded__details h3 {
    font-family: 'Avenir', 'Avenir Next', sans-serif;
    font-weight: 700;
    font-size: 0.75rem;
    color: #000000;
    text-transform: uppercase;
    letter-spacing: 0.1em;
    margin-bottom: 1.2rem;
}

.accordion-expanded__details .description {
    font-family: 'Avenir', 'Avenir Next', sans-serif;
    font-weight: 400;
    font-size: 1rem;
    color: #374151;
    line-height: 1.6;
    margin-bottom: 0.6rem;
}

.accordion-expanded__details .date {
    font-family: 'Avenir', 'Avenir Next', sans-serif;
    font-weight: 400;
    font-size: 0.85rem;
    color: #6b7280;
    margin-bottom: 1.2rem;
}

.accordion-expanded__details .contact-button {
    background-color: #000000;
    color: #ffffff;
    border: none;
    padding: 0.6rem 1.2rem;
    font-family: 'Avenir', 'Avenir Next', sans-serif;
    font-weight: 700;
    font-size: 0.65rem;
    text-transform: uppercase;
    letter-spacing: 0.05em;
    cursor: pointer;
    transition: all 0.3s ease;
    align-self: flex-start;
}

.accordion-expanded__details .contact-button:hover {
    background-color: #374151;
}

/* Hide the old content structure */
.accordion-content {
    display: none;
}

/* Responsive Accordion Styles */
@media (max-width: 768px) {
    .services-accordion {
        height: auto;
        min-height: 60vh;
    }
    
    .accordion-container {
        flex-direction: column;
        height: auto;
        min-width: 180px; /* Ensures all 3 tabs are visible on mobile */
    }
    
    .accordion-item {
        flex: 0 0 60px;
        min-height: 60px;
        min-width: 60px;
        border-right: none;
        border-bottom: 1px solid #e5e7eb;
    }
    
    .accordion-item:last-child {
        border-bottom: none;
    }
    
    .accordion-item.active {
        flex: 1;
        min-height: 320px;
        min-width: 0;
    }
    
    .accordion-tab {
        writing-mode: initial;
        text-orientation: initial;
        transform: none;
        padding: 0.6rem 0;
    }
    
    .accordion-tab__title {
        font-size: 18px;
        letter-spacing: 1px;
    }
    
    .accordion-item__expanded {
        flex-direction: column;
    }
    
    .accordion-expanded__image {
        flex: 0 0 60%;
    }
    
    .accordion-expanded__details {
        flex: 0 0 40%;
        padding: 1.2rem 1rem;
        justify-content: flex-start;
        align-items: flex-start;
        gap: 0.8rem;
    }
    
    .accordion-category-title {
        font-size: 1rem;
        margin-bottom: 0.8rem;
        letter-spacing: 0.1em;
    }
    
    .accordion-expanded__details h3 {
