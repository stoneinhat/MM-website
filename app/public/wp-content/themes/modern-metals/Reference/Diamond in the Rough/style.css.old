.intro-image-section {
    flex: 1;
    position: relative;
}

.intro-image-section img {
    width: 100%;
    height: auto;
    object-fit: cover;
}

.intro-image-overlay {
    position: absolute;
    bottom: 30px;
    left: 30px;
    background: rgba(0, 0, 0, 0.45);
    color: white;
    padding: 25px 30px;
    max-width: 280px;
    border: 2px solid #ffffff;
    backdrop-filter: blur(15px);
}

.intro-image-overlay h3 {
    font-family: 'Nunito Sans', sans-serif;
    font-size: 1.5rem;
    font-weight: 700;
    margin: 0 0 10px 0;
    letter-spacing: 1px;
}

.intro-image-overlay p {
    font-family: 'Nunito Sans', sans-serif;
    font-size: 1rem;
    margin: 0;
    opacity: 0.9;
}

.intro-content-section {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    padding-left: 20px;
}

.intro-content-section h3 {
    font-family: 'Nunito Sans', sans-serif;
    font-size: 2.5rem;
    font-weight: 700;
    line-height: 1.2;
    color: #333;
    margin: 0 0 30px 0;
}

.intro-content-section p {
    font-family: 'Nunito Sans', sans-serif;
    font-size: 1rem;
    line-height: 1.8;
    color: #666;
    margin: 0 0 20px 0;
}

.steel-works-contact-btn {
    background: #333;
    color: white;
    border: none;
    padding: 15px 30px;
    font-family: 'Nunito Sans', sans-serif;
    font-size: 0.9rem;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 1px;
    cursor: pointer;
    transition: all 0.3s ease;
    border-radius: 4px;
    margin-top: 20px;
    align-self: flex-start;
}

.steel-works-contact-btn:hover {
    background: #555;
    transform: translateY(-2px);
}

/* Responsive Design for Steel Works */
@media (max-width: 1024px) {
    #steel-works-hero .hero-title {
        font-size: 3rem;
    }
    
    #steel-works-hero {
        height: 45vh;
        min-height: 350px;
    }
    
    .steel-works-intro-top {
        flex-direction: column;
        gap: 30px;
        margin-bottom: 60px;
        align-items: center;
    }
    
    .intro-divider {
        width: 80px;
        height: 2px;
    }
    
    .intro-large-text h2 {
        font-size: 2.5rem;
        text-align: center;
    }
    
    .intro-small-text {
        text-align: center;
        margin-top: 0;
    }
    
    .steel-works-intro-bottom {
        flex-direction: column;
        gap: 40px;
    }
    
    .intro-content-section {
        padding-left: 0;
    }
    
    .intro-content-section h3 {
        font-size: 2rem;
    }
}

@media (max-width: 768px) {
    #steel-works-hero .hero-title {
        font-size: 2.8rem;
        letter-spacing: 0.25em;
        margin-bottom: 1.2rem;
    }
    
    #steel-works-hero {
        height: 40vh;
        min-height: 300px;
    }
    
    #steel-works-hero .hero-contact-btn {
        padding: 0.7rem 1.8rem;
        font-size: 0.8rem;
        letter-spacing: 0.1em;
    }
    
    .steel-works-introduction {
        padding: 60px 0;
    }
    
    .steel-works-intro-top {
        margin-bottom: 40px;
        gap: 20px;
    }
    
    .intro-divider {
        width: 60px;
    }
    
    .intro-large-text h2 {
        font-size: 2rem;
    }
    
    .intro-content-section h3 {
        font-size: 1.8rem;
    }
    
    .intro-image-overlay {
        bottom: 20px;
        left: 20px;
        padding: 20px;
        max-width: 250px;
    }
    
    .intro-image-overlay h3 {
        font-size: 1.3rem;
    }
}

@media (max-width: 480px) {
    #steel-works-hero .hero-title {
        font-size: 2.2rem;
        letter-spacing: 0.2em;
        margin-bottom: 1rem;
    }
    
    #steel-works-hero {
        height: 35vh;
        min-height: 280px;
    }
    
    #steel-works-hero .hero-subtitle {
        font-size: 0.95rem;
        letter-spacing: 0.06em;
        margin-bottom: 2rem;
        padding: 0 1rem;
    }
    
    #steel-works-hero .hero-contact-btn {
        padding: 0.6rem 1.5rem;
        font-size: 0.75rem;
        letter-spacing: 0.08em;
    }
    
    .steel-works-introduction {
        padding: 40px 0;
    }
    
    .steel-works-intro-top {
        gap: 15px;
    }
    
    .intro-divider {
        width: 50px;
    }
    
    .intro-large-text h2 {
        font-size: 1.8rem;
    }
    
    .intro-content-section h3 {
        font-size: 1.5rem;
    }
    
    .intro-image-overlay {
        bottom: 15px;
        left: 15px;
        padding: 15px;
        max-width: 200px;
    }
    
    .intro-image-overlay h3 {
        font-size: 1.1rem;
    }
    
    .intro-image-overlay p {
        font-size: 0.9rem;
    }
    
    .steel-works-contact-btn {
        width: 100%;
        text-align: center;
    }
} 
